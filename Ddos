    username = input(" \x1b[38;5;200m‚óèUsername‚ñ∫\x1b[38;5;226m")
    password = getpass.getpass(prompt=' \x1b[38;5;200m‚óèPassword‚ñ∫\x1b[38;5;226m')
    if username != user or password != passwd:
        print("")
        print(" \033[31m</> PASSWORD ERROR!...")
        sys.exit(1)
    elif username == user and password == passwd:
        print(" \033[32m</> PASSWORD GOOD!:) ")
        time.sleep(0.1)
        loading()
        main()

